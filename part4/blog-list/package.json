{
  "name": "blog-list",
  "version": "1.0.0",
  "description": "This is a blog list app",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node index.js",
    "dev": "cross-env NODE_ENV=development nodemon index.js",
    "test": "cross-env NODE_ENV=test node --test",
    "test:api": "cross-env NODE_ENV=test npm run test -- tests/api_bloglist.test.js",
    "test:api-get": "cross-env NODE_ENV=test npm run test:api --test-name-pattern='GET'",
    "test:api-post": "cross-env NODE_ENV=test npm run test:api --test-name-pattern='POST'",
    "test:api-put": "cross-env NODE_ENV=test npm run test:api --test-name-pattern='PUT'",
    "test:api-delete": "cross-env NODE_ENV=test npm run test:api --test-name-pattern='DELETE'"
  },
  "author": "Aggiovato",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-async-errors": "^3.1.1",
    "lodash": "^4.17.21",
    "mongoose": "^8.9.1"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "nodemon": "^3.1.9",
    "supertest": "^7.0.0"
  }
}
